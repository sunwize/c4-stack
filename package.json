{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "npx nuxi typecheck",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:auth": "npx @better-auth/cli generate --config server/utils/auth.ts --output server/database/schemas/users.ts --y"
  },
  "dependencies": {
    "@nuxt/image": "^1.10.0",
    "@nuxt/ui": "^3.2.0",
    "@orpc/client": "^1.7.2",
    "@orpc/server": "^1.7.2",
    "@orpc/vue-query": "^1.7.2",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/vue-query": "^5.83.0",
    "@vueuse/core": "^13.5.0",
    "better-auth": "^1.3.2",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "nuxt": "^4.0.0",
    "postgres": "^3.4.7",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.7.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vue-tsc": "^2.2.10"
  }
}
